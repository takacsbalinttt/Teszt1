@model Hotcakes.Modules.Core.Models.SideMenuViewModel
<div class="row-fluid hc-top-weekly-sellers">
    <div class="col-xs-12">
        <h2>@Model.Title</h2>
        <ol>
            @foreach (var item in Model.Items)
            {
                <li><a href="@item.Url" title="@item.Title">@item.Name</a></li>
            }
        </ol>
    </div>
</div>
<div class="clearfix"></div>